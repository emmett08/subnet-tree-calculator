{
  "name": "@neuralsea/subnet-tree-calculator",
  "version": "1.0.0",
  "description": "Themeable React + TypeScript subnet calculator (IPv4 + IPv6) rendered as an interactive SVG binary tree using D3.",
  "keywords": [
    "subnet",
    "cidr",
    "ipv4",
    "ipv6",
    "react",
    "d3",
    "svg",
    "calculator",
    "networking"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "assets"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite --config demo/vite.config.ts",
    "build": "tsup && npm run build:css",
    "build:css": "node ./scripts/copy-css.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "demo:build": "vite build --config demo/vite.config.ts",
    "demo:preview": "vite preview --config demo/vite.config.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "d3": "^7.9.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^8.6.14",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}
